	keywords = {circRNAs, RNA-seq},
	pages = {2396--2409},
	file = {Full Text:/home/nico/Zotero/storage/ERQXQHZC/Wang et al. - 2021 - Identification and characterization of circular RN.pdf:application/pdf;ScienceDirect Snapshot:/home/nico/Zotero/storage/6X83M45T/S0022030220309747.html:text/html},
