	file = {Full Text PDF:/home/nico/Zotero/storage/7FGSQCHY/Chuang et al. - 2016 - NCLscan accurate identification of non-co-linear transcripts (fusion, trans-splicing and circular R.pdf:application/pdf;Snapshot:/home/nico/Zotero/storage/FD8UMFNJ/2502630.html:text/html},
}

@article{zhang_microarray_2017,
	title = {Microarray analysis of circular {RNA} expression patterns in polarized macrophages},
