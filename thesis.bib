@article{deng_er-36-mediated_2014,
	title = {{ER}-α36-{Mediated} {Rapid} {Estrogen} {Signaling} {Positively} {Regulates} {ER}-{Positive} {Breast} {Cancer} {Stem}/{Progenitor} {Cells}},
