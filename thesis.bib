	month = jun,
	year = {2020},
	note = {Publisher: Wolters Kluwer},
	pages = {1887--1896},
	file = {PubMed Central Full Text PDF:/home/nico/Zotero/storage/Z33MYEXT/Modi et al. - 2020 - Antitumor Activity and Safety of Trastuzumab Deruxtecan in Patients With HER2-Low–Expressing Advance.pdf:application/pdf},
