	note = {Publisher: Proceedings of the National Academy of Sciences},
	pages = {21737--21742},
	file = {Full Text PDF:/home/nico/Zotero/storage/VBVZINPN/Fillmore et al. - 2010 - Estrogen expands breast cancer stem-like cells through paracrine FGFTbx3 signaling.pdf:application/pdf},
}

