	pages = {53},
	file = {Full Text PDF:/home/nico/Zotero/storage/A4QE8JIP/Fraboulet et al. - 2024 - Cirscan a shiny application to identify different.pdf:application/pdf;Snapshot:/home/nico/Zotero/storage/ITBT2689/s12859-024-05668-y.html:text/html},
}

@article{hoffmann_circrna-sponging_2023,
